{
  "name": "TinyVid",
  "version": "0.15.6",
  "main": "./dist/main.js",
  "author": "Kamua<hello@kamua.com>",
  "description": "Simple to use video trimmer & compressor",
  "scripts": {
    "dev:electron": "NODE_ENV=development webpack --config webpack.electron.config.js --mode development --watch",
    "dev:prod:electron": "NODE_ENV=production webpack --config webpack.electron.config.js --mode production --watch",
    "dev:react": "NODE_ENV=development webpack-dev-server --config webpack.react.config.js --mode development",
    "ffbin": "ffbinaries ffprobe ffmpeg -p linux-64 -o bin/linux-64 && ffbinaries ffprobe ffmpeg -p windows-64 -o bin/windows-64 && ffbinaries ffprobe ffmpeg -p osx-64 -o bin/osx-64",
    "build:js-win": "yarn install && set NODE_ENV=production && webpack --config webpack.electron.config.js --mode production && set NODE_ENV=production && webpack --config webpack.react.config.js --mode production",
    "build:js": "yarn install && NODE_ENV=production webpack --config webpack.electron.config.js --mode production && NODE_ENV=production webpack --config webpack.react.config.js --mode production",
    "build:win": "yarn build:js && electron-builder --win -p always",
    "build:linux": "yarn build:js && electron-builder --linux -p always",
    "build:mac": "yarn build:js && electron-builder --mac -p always"
  },
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@sentry/cli": "^1.63.2",
    "@types/color": "^3.0.1",
    "@types/node": "^14.0.13",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/uuid": "^8.0.0",
    "babel-loader": "^8.1.0",
    "color": "^3.1.2",
    "css-loader": "^3.5.3",
    "dotenv": "^8.2.0",
    "electron": "^9.0.0",
    "electron-builder": "^22.10.5",
    "electron-builder-squirrel-windows": "^22.10.5",
    "electron-download": "^4.1.1",
    "electron-notarize": "^1.0.0",
    "electron-updater": "^4.3.8",
    "ffbinaries": "^1.1.4",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "moment": "^2.26.0",
    "nouislider": "^14.5.0",
    "nouislider-react": "^3.3.7",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-number-format": "^4.4.1",
    "source-map-loader": "^1.0.0",
    "style-loader": "^1.2.1",
    "url-loader": "^4.1.0",
    "uuid": "^8.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.15",
    "@sentry/electron": "^2.4.0",
    "@sentry/integrations": "^6.2.5",
    "@types/classnames": "^2.2.11",
    "@types/throttle-debounce": "^2.1.0",
    "await-lock": "^2.1.0",
    "classnames": "^2.3.0",
    "electron-log": "^4.2.2",
    "ffmpeg-progress-wrapper": "^2.0.0",
    "mobx": "^6.2.0",
    "mobx-react": "^7.1.0",
    "mobx-utils": "^6.0.4",
    "otplib": "^12.0.1",
    "react-merge-refs": "^1.1.0",
    "source-map-support": "^0.5.19",
    "throttle-debounce": "^3.0.1",
    "ts-loader": "^8.1.0",
    "typescript": "^4.2.4"
  },
  "build": {
    "appId": "com.kamua.tinyvid",
    "productName": "TinyVid",
    "artifactName": "${name}-${os}-${arch}-${version}.${ext}",
    "asar": true,
    "files": [
      "dist",
      "icon.png"
    ],
    "afterSign": "build/scripts/notarize.js",
    "publish": [
      {
        "provider": "s3",
        "bucket": "quicktrim-public-update",
        "path": "download/${channel}/${os}/${arch}"
      }
    ],
    "directories": {
      "buildResources": "build",
      "output": "out"
    },
    "mac": {
      "category": "public.app-category.video",
      "target": [
        "dir",
        "dmg",
        "zip"
      ],
      "icon": "icon.icns",
      "darkModeSupport": true,
      "extraFiles": [
        "bin/osx-64"
      ],
      "hardenedRuntime": true,
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist"
    },
    "win": {
      "target": [
        "dir",
        "nsis",
        "zip"
      ],
      "sign": "build/scripts/eSigner.js",
      "icon": "icon.ico",
      "extraFiles": [
        "bin/windows-64"
      ],
      "certificateSubjectName": "Findie Development Limited"
    },
    "nsis": {
      "oneClick": false,
      "installerIcon": "icon.ico",
      "uninstallerIcon": "icon.ico",
      "allowToChangeInstallationDirectory": true
    },
    "linux": {
      "target": [
        "dir",
        "appImage",
        "zip"
      ],
      "extraFiles": [
        "bin/linux-64"
      ],
      "executableName": "TinyVid",
      "icon": "icon.png"
    }
  }
}
